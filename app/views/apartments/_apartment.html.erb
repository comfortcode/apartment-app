<div class="col-md-4 col-sm-6 col-xs-12">
    <div class="property-container">
        <div class="property-image">
            <img src="data:image/jpeg;base64," alt="Ramat Eshkol Apartment">
                <div class="property-price">
                  <h4><%= apartment.rental_type.name if apartment.rental_type %> Rental</h4>
                      <span><%= number_to_currency apartment.price, unit: apartment.currency.symbol.html_safe, precision: 0 %><small>/<%= apartment.price_time.name %></small></span>
                </div>
                <div class="property-status">
                    <span><%= apartment.from_date_display %></span>
                </div>
        </div>
        <div class="property-features">
            <span><i class="fa fa-hdd-o"></i> <%= pluralize(apartment.beds_int, "Bedroom") %></span>
            <span><i class="fa fa-male"></i> <%= pluralize(apartment.baths_int, "Bathroom") %></span>
        </div>
        <div class="property-content">
            <h3><%= link_to apartment.street, apartment%></a> <small>Apartment in <%= apartment.area.name %></small></h3>
            <% if apartment.user == current_user %>
                <%= link_to 'View', apartment %> / 
                <%= link_to 'Edit', edit_apartment_path(apartment) %> / 
                <%= link_to 'Delete', apartment, method: :delete, data: { confirm: 'Are you sure you want to permanently delete this apartment listing?' } %>
            <% else %>
                <%= link_to 'View Details', apartment %>
            <% end %>
        </div>
    </div>
</div>
